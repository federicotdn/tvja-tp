#version 120attribute vec4 a_position;attribute vec4 a_normal;attribute vec2 a_texCoord0;uniform mat4 u_mvp;varying vec4 v_position;varying vec4 v_normal;varying vec2 v_texCoords;void main(){    v_texCoords = a_texCoord0;    v_position = a_position;    v_normal = a_normal;    gl_Position =  u_mvp * a_position;}